begin
    let testVar be 1234
    let result be 5 * (15 / 4) + 7
    let count be 0
    let negativeNumber be -2

    # I am a comment. Don't interpret me!
    
    loop while count isLessThan 5
        let countString be ...
            (concat: "Current count is: " count)

        print: countString
        print: "This is a test of escaping '\"'."

        update count to (count + 1)
    end

    declare function printResult withParameters count
        if count isEqualTo 4
            print: "testing"
        else if false
            print: "this is also not supposed to run"
        else
            print: "This should print"
        end
    end
end